#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/prisma-generator/dist/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/prisma-generator/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/prisma-generator/dist/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/prisma-generator/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules/@electric-sql/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/@electric-sql+prisma-generator@1.1.5/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@electric-sql/prisma-generator/dist/bin.js" "$@"
else
  exec node  "$basedir/../@electric-sql/prisma-generator/dist/bin.js" "$@"
fi
