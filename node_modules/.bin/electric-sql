#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/dist/cli/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/dist/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/dist/cli/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/dist/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules/electric-sql/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/electric-sql@0.12.1_react-dom@19.1.0_react@19.1.0__react-native@0.81.5_@babel+core@7.28_915e1cc2b6f8b2a00ce474e99b8fcd30/node_modules:/Users/webdev/Desktop/GitHubRepo/SHR26/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../electric-sql/dist/cli/main.js" "$@"
else
  exec node  "$basedir/../electric-sql/dist/cli/main.js" "$@"
fi
